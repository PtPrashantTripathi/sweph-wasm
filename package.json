        "rollup": "latest",
        "rollup-plugin-copy": "latest",
        "tslib": "latest",
        "typescript": "latest",
        "typescript-eslint": "latest",
        "vitest": "latest"
    },
    "engines": {
        "node": ">=22"
    },
    "typeScriptVersion": "latest"
